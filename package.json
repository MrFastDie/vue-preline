    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/runtime-dom": "^3.3.4",
    "vite": "^4.4.9",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.35.0",
    "vite-plugin-dts": "^3.5.3"
  }
